<?xml version="1.0" encoding="iso-8859-1"?>
<!-- <!DOCTYPE Uintah_specification SYSTEM "input.dtd"> -->
<!-- @version: Updated 7/31/00-->

<Uintah_specification>

   <Meta>
       <title>Arenisca_Verification_Test_11_Uniaxial_Strain_J2_Plasticity</title>
   </Meta>

   <SimulationComponent type="mpm" />

   <Time>
       <maxTime>8</maxTime>
       <initTime>0.0</initTime>
       <delt_min>1e-3</delt_min>
       <delt_max>1e-3</delt_max>
       <timestep_multiplier>1</timestep_multiplier>
   </Time>

   <DataArchiver>
       <filebase> AreniscaTest_11_UniaxialStrainJ2plasticity.uda </filebase>
       <outputInterval>.01</outputInterval>
       <save label="p.x"/>
       <save label="p.velocity"/>
       <save label="p.volume"/>
       <save label="p.stress"/>
       <save label="p.deformationGradient"/>
       <save label="p.particleID"/>
       <save label="g.stressFS"/>
       <save label = "p.evp"/>
       <save label = "p.eve"/>
    </DataArchiver>

    <MPM>
      	<time_integrator>           explicit   </time_integrator>
      	<interpolator>              gimp       </interpolator>
        <withColor>                 false      </withColor>
        <UsePrescribedDeformation>  true       </UsePrescribedDeformation>
        <PrescribedDeformationFile>     inputs/MPM/Arenisca/AreniscaTest_11_PrescribedDeformation.inp  </PrescribedDeformationFile>
        <minimum_subcycles_for_F>       -2     </minimum_subcycles_for_F>
    </MPM>

    <PhysicalConstants>
       <gravity>[0,0,0]</gravity>
    </PhysicalConstants>

    <MaterialProperties>
       <MPM>
         <material>
           <include href="../Arenisca3sandstone_undrained.xml"/>

              <geom_object>
                <box label="Elem">
                  <min>[ 1.0, 1.0, 1.0]</min>
                  <max>[ 1.5, 1.5, 1.5]</max>
                </box>
                <res>[1,1,1]</res>
                <velocity>[0.0,0.0,0]</velocity>
                <temperature>22</temperature>
              </geom_object>
         </material>
       </MPM>
    </MaterialProperties>
       
    <Grid>
       <BoundaryConditions>
           <Face side = "x-">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>
           <Face side = "x+">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>
           <Face side = "y-">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>                  
          <Face side = "y+">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>
           <Face side = "z-">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>
           <Face side = "z+">
             <BCType id = "all" label = "Velocity" var = "Dirichlet"><value>[0.0,0.0,0.0]</value></BCType>
           </Face>                            
       </BoundaryConditions>
       <Level>
           <Box label="1">
              <lower>[ 0.0, 0.0, 0.0]</lower>
              <upper>[ 2.5, 2.5, 2.5]</upper>
              <patches>[1,1,1]</patches>
              <extraCells>[1,1,1]</extraCells>
           </Box>
           <spacing>[0.5,0.5,0.5]</spacing>
       </Level>
    </Grid>

</Uintah_specification>
